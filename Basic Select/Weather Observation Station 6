# Problem: Query the list of CITY names starting with vowels (i.e., a, e, i, o, or u) from STATION. 
# Your result cannot contain duplicates.

select distinct city from station
where city like "a%" 
union
select distinct city from station
where city like "e%" 
union
select distinct city from station
where city like "i%" 
union
select distinct city from station
where city like "o%" 
union
select distinct city from station
where city like "u%";
